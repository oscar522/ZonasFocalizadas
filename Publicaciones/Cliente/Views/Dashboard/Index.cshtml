            // Grafica de exp por municipio
            function GrapExpPorMunicipio(NameDepto) {

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("CountDeptoMuni")',
                    dataType: 'json',
                    success: function (s) {
                        var TotalExp = 0 ;
                        var dataList = [];
                        $.each(s, function (i, obj) {

                            if(NameDepto != "N_A"){

                                if(NameDepto.toLowerCase() == obj.NOMBRE_DEPTO.toLowerCase()){
                                    var res = obj.NOMBRE_PAIS.split("-");
                                    var ExpCountIndv = parseInt(res[0]);
                                    var dataEntity = {name : obj.Nombre.toLowerCase(), data: [ExpCountIndv], color:'#068C9B' };
                                    dataList.push(dataEntity);
                                }

                            }else{
                                var res = obj.NOMBRE_PAIS.split("-");
                                var ExpCountIndv = parseInt(res[0]);
                                var dataEntity = {name :obj.Nombre.toLowerCase() , data: [ExpCountIndv], color:'#068C9B' };
                                dataList.push(dataEntity);
                            }
                        });
                        Highcharts.chart('container-Tipificacion', {
                            chart: {
                                type: 'column'
                            },
                            title: {
                                text: ''
                            },
                            subtitle: {
                                text: 'Expedientes por municipio'
                            },
                            xAxis: {
                                categories: [''],
                            },
                            yAxis: {
                                title: {
                                    text: 'Numero de expedientes'
                                }
                            },
                            credits: {
                                enabled: false
                            },
                            series: dataList,
                        });
                    },
                    error: function(e){
                        oTable.fnClearTable();
                    }
                });
            }
            
            GrapExpPorMunicipio("N_A");

            // Grafica de Tipificacion
            function GrapGrupoTipificacion(NameDepto ) {
                var NameDeptoForm = "N_A";
