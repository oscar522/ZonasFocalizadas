                            subtitle: {
                                text: 'Tipificacion por grupos'
                            },
                            xAxis: {
                                categories: [''],
                            },
                            yAxis: {
                                title: {
                                    text: 'Numero de expedientes'
                                }
                            },
                            credits: {
                                enabled: false
                            },
                            series: dataList,
                        });
                    },
                    error: function(e){
                        oTable.fnClearTable();
                    }
                });
            }
            
